{
  "name": "jet",
  "version": "0.8.0",
  "description": "Bring your React Native JS code into Node.js and test it mock-free and native code free. Perfect for React Native module developers wanting full CI.",
  "author": {
    "name": "Mike Diarmid",
    "email": "mike.diarmid@gmail.com",
    "url": "http://github.com/Salakar/"
  },
  "main": "./lib/node",
  "react-native": "./lib/react-native",
  "keywords": [
    "react-native",
    "react native module",
    "detox",
    "debugger",
    "mocha",
    "coverage",
    "node",
    "nodejs",
    "protocol",
    "batched bridge",
    "istanbul",
    "ci",
    "integration",
    "test",
    "testing"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint ./lib/runner/* ",
    "lint-fix": "eslint --fix ./lib/runner/*"
  },
  "optionalDependencies": {
    "detox": "^18.20.3",
    "mocha": "^9.1.1"
  },
  "peerDependencies": {
    "detox": "^12.5.0",
    "mocha": "^5.2.0",
    "react-native": "+"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "error-stack-parser": "^2.0.2",
    "istanbul-lib-coverage": "^3.0.0",
    "should": "^13.2.1",
    "should-sinon": "0.0.6",
    "sinon": "^11.1.2",
    "source-map": "^0.7.3",
    "tinyqueue": "^2.0.3",
    "ws": "^8.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.4",
    "@babel/plugin-proposal-private-property-in-object": "^7.15.4",
    "@types/lodash": "^4.14.172",
    "@types/react-native": "^0.64.14",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.7.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-native": "^0.66.0-rc.1",
    "typescript": "^4.4.2"
  },
  "lint-staged": {
    "lib/runner/**/*.[jsx?|tsx?]": [
      "eslint --fix",
      "git add"
    ],
    "server/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  }
}
