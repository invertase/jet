{
  "name": "jet",
  "version": "0.8.0",
  "license": "Apache-2.0",
  "description": "Auto-generate a test app and auto-run simple API tests mock-free. Perfect for React Native module developers wanting full CI.",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "url": "https://github.com/invertase/jet",
  "contributors": [
    {
      "name": "Mike Diarmid",
      "email": "mike.diarmid@gmail.com",
      "url": "https://github.com/Salakar/"
    },
    {
      "name": "Mike Hardy",
      "email": "github@mikehardy.net",
      "url": "https://github.com/mikehardy/"
    }
  ],
  "keywords": [
    "react-native",
    "react native module",
    "detox",
    "mocha",
    "coverage",
    "istanbul",
    "ci",
    "integration",
    "test",
    "testing"
  ],
  "scripts": {
    "validate:all:js": "eslint ./lib/runner --ext .js,.jsx,.ts,.tsx",
    "validate:all:js-fix": "eslint --fix ./lib/runner --ext .js,.jsx,.ts,.tsx",
    "validate:all:ts": "tsc --noEmit",
    "build": "tsc",
    "build:clean": "rimraf rimraf android/build && rimraf ios/build && rimraf dist",
    "build:watch": "tsc --watch",
    "prepare": "yarn build",
    "precommit": "lint-staged"
  },
  "optionalDependencies": {
    "detox": "^18.22.1",
    "mocha": "^9.1.2"
  },
  "peerDependencies": {
    "detox": "^12.5.0",
    "mocha": "^5.2.0",
    "react": "+",
    "react-native": "+"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "error-stack-parser": "^2.0.2",
    "istanbul-lib-coverage": "^3.0.1",
    "should": "^13.2.1",
    "should-sinon": "0.0.6",
    "sinon": "^11.1.2",
    "source-map": "^0.7.3",
    "tinyqueue": "^2.0.3",
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/plugin-proposal-private-property-in-object": "^7.15.4",
    "@types/lodash": "^4.14.175",
    "@types/react-native": "^0.65.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "lint-staged": "^11.2.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-native": "^0.66.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "lib/runner/**/*.[jsx?|tsx?]": [
      "eslint --fix",
      "git add"
    ],
    "server/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  }
}
